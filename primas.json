{
	"apps": [
		{
			"exec_mode": "cluster_mode",
			"instances": 4,
			"name": "primas",
			"script": "bin/www",
			"node_args": "--max_old_space_size=256 --experimental-modules",
			"max_memory_restart" : "200M",
			"log_date_format": "MM-DD HH:mm",
			"merge_logs": true,
			"watch": true,
			"env": {
				"NODE_ENV": "production"
			},
			"env_development": {
				"NODE_ENV": "development",
				"DEBUG": "*"
			}
		}
	]
}
